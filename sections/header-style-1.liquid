{% comment %} Add the below schema on inside the "blocks": []  {% endcomment %}
{
  "type": "megamenu",
  "name": "MegaMenu",
  "settings": [
    {
      "type": "text",
      "id": "title",
      "label": "Menu Title",
      "info": "Use Exact string as header"
    },
    {
      "type": "select",
      "id": "column",
      "label": "Display Megamenu Column",
      "options": [
        {
          "value": "count_2",
          "label": "Two Column"
        },
        {
          "value": "count_3",
          "label": "Three Column"
        },
        {
          "value": "count_4",
          "label": "Four Column"
        }
      ],
      "default": "count_3"
    },
    {
        "type":"image_picker",
        "id":"image",
        "label": "Image",
        "info": "Use 480x480 sized image."
    },
    {
        "type":"text",
        "id":"link_text",
        "label":"Link text"
    },
    {
        "type":"url",
        "id":"link_url",
        "label":"URL"
    }
  ]
}

